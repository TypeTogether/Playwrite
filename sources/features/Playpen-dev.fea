feature calt {
	lookup POSITIONS {
		lookupflag IgnoreMarks;
		#
		sub @UC_CursiveHasIni' @posBase by @UC_CursiveIni;
		sub @UC_CursiveIni @posBase' @posBase by @posMed;
		sub @UC_CursiveIni @posBase' by @posFin;
		#
		sub @UC_DecorativeHasIni' @posBase by @UC_DecorativeIni;
		sub @UC_DecorativeIni @posBase' @posBase by @posMed;
		sub @UC_DecorativeIni @posBase' by @posFin;
		#
		sub @posIni @posBase' @posBase by @posMed;
		sub [@posIni @posMed] @posBase' @posBase by @posMed;
		sub @posBase' @posBase by @posIni;
		sub [@posIni @posMed] @posBase' by @posFin;
		#
	} POSITIONS;

	# deal with special pairs
	lookup TRACKBACK {
		lookupflag IgnoreMarks;
		# MOD
		sub [b.mod.ini b.mod.med f.mod.ini f.mod.med g.mod.ini g.mod.med j.mod.ini j.mod.med p.mod.ini p.mod.med q.mod.ini q.mod.med s.mod.ini s.mod.med y.mod.ini y.mod.med ij.mod.ini ij.mod.med] [e.mod.med e.mod.fin]' by [e.mod.med_b_ e.mod.fin_b_];
		sub [a.mod.ini a.mod.med c.mod.ini c.mod.med d.mod.ini d.mod.med e.mod.ini e.mod.med e.mod.med_b_ e.mod.med_n_ e.jmc.med_g_ h.mod.ini h.mod.med i.mod.ini i.mod.med k.mod.ini k.mod.med k.ful.ini k.ful.med l.mod.ini l.mod.med m.mod.ini m.mod.med n.mod.ini n.mod.med t.mod.ini t.mod.med u.mod.ini u.mod.med x.mod.ini x.mod.med z.mod.ini z.mod.med A.cur.ini A.cur_locl.ini A.dec_locl.ini G.cur.ini N.cur.ini Q.cur.ini Q.cur_locl.ini T.cur.ini T.cur_locl.ini X.cur.ini X.dec.ini X.dec_locl.ini Z.cur.ini Z.dec.ini x.ful.ini x.ful.med] [e.mod.med e.mod.fin]' by [e.mod.med_n_ e.mod.fin_n_];
		sub [o.mod.ini o.mod.med r.mod.ini r.mod.med v.mod.ini v.mod.med w.mod.ini w.mod.med] [f.mod.med f.mod.fin f.mod_au.med f.mod_au.fin f.jmc.med f.jmc.fin t.mod.med t.mod.fin]' by [f.mod.med_o_ f.mod.fin_o_ f.mod_au.med_o_ f.mod_au.fin_o_ f.jmc.med_o_ f.jmc.fin_o_ t.mod.med_o_ t.mod.fin_o_];
		# JMC
		sub [b.jmc.ini b.jmc.med f.jmc.ini f.jmc.med p.jmc.ini p.jmc.med q.jmc.ini q.jmc.med s.jmc.ini s.jmc.med] [e.mod.med e.mod.fin]' by [e.mod.med_n_ e.mod.fin_n_];
		sub [f.jmc.ini f.jmc.med] [e.mod.med e.mod.fin]' by [e.mod.med_b_ e.mod.fin_b_];
		sub [g.jmc.ini g.jmc.med j.jmc.ini j.jmc.med y.jmc.ini y.jmc.med] [e.mod.med e.mod.fin]' by [e.jmc.med_g_ e.jmc.fin_g_];
		# FUL
		sub [o.mod.ini o.mod.med b.ful.ini b.ful.med v.ful.ini v.ful.med w.ful.ini w.ful.med b.lop.ini b.lop.med r.cnt.ini r.cnt.med r.cnt.med_n_ v.cnt.ini v.cnt.med w.cnt.ini w.cnt.med v.cnt.med_n_ w.cnt.med_n_ @bmlp_SRC o.mlp.ini o.mlp.med] [e.ful.med e.ful.fin]' by [e.ful.med_o_ e.ful.fin_o_];
		sub [c.mod.ini c.mod.med f.ful_de.ini f.ful_de.med g.jmc.ini g.jmc.med j.jmc.ini j.jmc.med y.jmc.ini y.jmc.med y.cnt.ini y.cnt.med y.cnt.med_n_ z.cnt.ini z.cnt.med z.ful.ini z.ful.med z.mlp.ini z.mlp.med T.cur.ini T.cur_locl.ini] [e.ful.med e.ful.fin]' by [e.ful.med_g_ e.ful.fin_g_];
		sub [@bmlp_SRC b.ful.ini b.ful.med b.lop.ini b.lop.med o.mod.ini o.mod.med r.cnt.ini r.cnt.med r.cnt.med_n_ v.cnt.ini v.cnt.med v.cnt.med_n_ v.ful.ini v.ful.med w.cnt.ini w.cnt.med w.cnt.med_n_ w.ful.ini w.ful.med] [f.mod_au.med f.mod_au.fin t.mod.med t.mod.fin]' by [f.mod_au.med_o_ f.mod_au.fin_o_ t.mod.med_o_ t.mod.fin_o_ ];
		# CNT
		@cStroke_SRC = [m.cnt.fin m.cnt.med n.cnt.fin n.cnt.med r.cnt.fin r.cnt.med v.cnt.fin v.cnt.med w.cnt.fin w.cnt.med y.cnt.fin y.cnt.med];
		@cStroke_TGT = [m.cnt.fin_n_ m.cnt.med_n_ n.cnt.fin_n_ n.cnt.med_n_ r.cnt.fin_n_ r.cnt.med_n_ v.cnt.fin_n_ v.cnt.med_n_ w.cnt.fin_n_ w.cnt.med_n_ y.cnt.fin_n_ y.cnt.med_n_];
		sub @eful_SRC @cStroke_SRC' by @cStroke_TGT;
